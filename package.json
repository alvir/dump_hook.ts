    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "postgres": "^3.4.5",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
